{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ESNext",
    "types": ["node"],
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true
  }
}
